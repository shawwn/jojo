(assert (eq (cond [(eq 1 0) 'A 'A])
            (when (eq 1 0) (begin 'A 'A))))

(assert (eq (cond [(eq 1 0) 'A 'A]
                  [(eq 2 2) 'B 'B]
                  [else 'C 'C])
            (if (eq 1 0) (begin 'A 'A)
              (if (eq 2 2) (begin 'B 'B)
                (begin 'C 'C)))))

(assert (eq (cond [(eq 1 0) 'A 'A]
                  [(eq 2 0) 'B 'B]
                  [else 'C 'C])
            (if (eq 1 0) (begin 'A 'A)
              (if (eq 2 0) (begin 'B 'B)
                (begin 'C 'C)))))

(assert (eq (cond [(eq 1 0) 'A 'A]
                  [(eq 2 0) 'B 'B]
                  [(eq 3 3) 'C 'C])
            (if (eq 1 0) (begin 'A 'A)
              (if (eq 2 0) (begin 'B 'B)
                (when (eq 3 3) (begin 'C 'C))))))
